  <servlet>
    <servlet-name>EmailServlet</servlet-name>
    <servlet-class>nl.hu.bep.friendspammer.web.EmailServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>HistoryServlet</servlet-name>
